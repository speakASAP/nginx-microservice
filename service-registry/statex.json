{
  "service_name": "statex",
  "service_path": "/home/statex/statex",
  "production_path": "/home/statex/statex",
  "domain": "statex.cz",
  "docker_compose_file": "docker-compose.blue.yml",
  "docker_project_base": "statex",
  "services": {
    "frontend": {
      "container_name_base": "statex-frontend",
      "port": 3000,
      "health_endpoint": "/",
      "health_timeout": 5,
      "health_retries": 3,
      "startup_time": 40
    },
    "submission-service": {
      "container_name_base": "statex-submission-service",
      "port": 8000,
      "health_endpoint": "/health",
      "health_timeout": 5,
      "health_retries": 3,
      "startup_time": 30
    },
    "user-portal": {
      "container_name_base": "statex-user-portal",
      "port": 8000,
      "health_endpoint": "/health",
      "health_timeout": 5,
      "health_retries": 3,
      "startup_time": 30
    },
    "content-service": {
      "container_name_base": "statex-content-service",
      "port": 8000,
      "health_endpoint": "/health",
      "health_timeout": 5,
      "health_retries": 3,
      "startup_time": 30
    },
    "ai-orchestrator": {
      "container_name_base": "statex-ai-orchestrator",
      "port": 8000,
      "health_endpoint": "/health",
      "health_timeout": 5,
      "health_retries": 3,
      "startup_time": 40
    },
    "api-gateway": {
      "container_name_base": "statex-api-gateway",
      "port": 80,
      "health_endpoint": "/health",
      "health_timeout": 5,
      "health_retries": 3,
      "startup_time": 20
    }
  },
  "shared_services": [
    "postgres",
    "redis"
  ],
  "infrastructure_compose_file": "docker-compose.infrastructure.yml",
  "infrastructure_project_name": "statex_infrastructure",
  "network": "nginx-network",
  "domains": {
    "statex.cz": {
      "services": ["frontend", "content-service", "submission-service", "user-portal", "ai-orchestrator", "api-gateway"]
    }
  }
}
